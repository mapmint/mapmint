[test]
 Title = Test connecting to a WMS or a WFS Data Store.
 Abstract = Test accessing to a WMS or a WFS Data Store.
 processVersion = 1
 storeSupported = true
 statusSupported = true
 serviceProvider = service
 serviceType = Python
 <DataInputs>
  [name]
   Title = The Data Store Name
   Abstract = The name of the Data Store to create.
   minOccurs = 1
   maxOccurs = 1
   <LiteralData>
    DataType = string
    <Default />
   </LiteralData>
  [url]
   Title = The Server Url
   Abstract = The URL to acces the WMS/WFS Server.
   minOccurs = 1
   maxOccurs = 1
   <LiteralData>
    DataType = string
    <Default />
   </LiteralData>
  [type]
   Title = The OGC Protocol
   Abstract = The type of OGC protocol to use WMS or WFS.
   minOccurs = 1
   maxOccurs = 1
   <LiteralData>
    DataType = string
    <Default />
   </LiteralData>
 </DataInputs>
 <DataOutputs>
  [Result]
   Title = The resulting message
   Abstract = The message providing informations about tests run to access the WMS or WFS server.
   <LiteralData>
    DataType = string
    <Default>
     UOM = meter
    </Default>
    <Supported>
     UOM = meter
    </Supported>
   </LiteralData>
 </DataOutputs>
